{
  "plog" : {
    "server" : {
      "tcp" : {
        "listeners" : [
          {
            "port" : 23456
          }
        ]
      },
      "udp" : {
        "listeners" : [
          {
            "port" : 23460,
            "handlers" : [
              {
                "provider" : "com.airbnb.plog.kafka.KafkaProvider",
                "default_topic" : "scram_events",
                "producer_config" : {
                  "compression" : {
                    "codec" : "snappy"
                  },
                  "queue" : {
                    "enqueue" : {
                      "timeout" : {
                        "ms" : 0
                      }
                    }
                  },
                  "producer" : {
                    "type" : "async"
                  },
                  "bootstrap" : {
                    "servers" : "localhost:9092"
                  },
                  "request" : {
                    "required" : {
                      "acks" : 1
                    }
                  }
                }
              }
            ]
          },
          {
            "port" : 23461,
            "handlers" : [
              {
                "provider" : "com.airbnb.plog.console.ConsoleOutputProvider"
              },
              {
                "provider" : "com.airbnb.plog.kafka.KafkaProvider",
                "default_topic" : "null",
                "producer_config" : {
                  "compression" : {
                    "codec" : "snappy"
                  },
                  "queue" : {
                    "enqueue" : {
                      "timeout" : {
                        "ms" : 0
                      }
                    }
                  },
                  "producer" : {
                    "type" : "async"
                  },
                  "bootstrap" : {
                    "servers" : "localhost:9092"
                  },
                  "request" : {
                    "required" : {
                      "acks" : 1
                    }
                  }
                }
              }
            ]
          }
        ]
      }
    }
  }
}